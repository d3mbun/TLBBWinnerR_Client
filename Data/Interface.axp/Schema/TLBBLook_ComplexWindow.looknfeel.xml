  <WidgetLook name="TLBB_Look_RewardItem">
    <PropertyDefinition name="VertLabelFormatting" initialValue="CentreAligned" />
    <PropertyDefinition name="HorzLabelFormatting" initialValue="CentreAligned" />
    <ImagerySection name="pushed">
      <ImageryComponent>
        <Area>
          <Dim type="LeftEdge">
            <AbsoluteDim value="0" />
          </Dim>
          <Dim type="TopEdge">
            <AbsoluteDim value="0" />
          </Dim>
          <Dim type="Width">
            <UnifiedDim scale="1" type="Width" />
          </Dim>
          <Dim type="Height">
            <UnifiedDim scale="1" type="Height" />
          </Dim>
        </Area>
        <Image imageset="CommonFrame" image="ActionButtonHover" />
        <VertFormat type="Stretched" />
        <HorzFormat type="Stretched" />
      </ImageryComponent>
    </ImagerySection>
    <StateImagery name="Normal">
      <Layer />
    </StateImagery>
    <StateImagery name="Hover">
      <Layer />
    </StateImagery>
    <StateImagery name="Pushed">
      <Layer>
        <Section section="pushed" />
      </Layer>
    </StateImagery>
    <StateImagery name="Disabled">
      <Layer />
    </StateImagery>
  </WidgetLook>
  <WidgetLook name="TLBB_Look_ComplexWindow_ParentWindow"></WidgetLook>
