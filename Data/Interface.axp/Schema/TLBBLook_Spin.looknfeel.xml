<?xml version="1.0" ?>
<Falagard>
	
	<!--
    ***************************************************
        Action Button
    ***************************************************
    -->
	<WidgetLook name="TLBB_Look_Spinner">
    <Child type="TLBB_ButtonNULL" nameSuffix="__auto_incbtn__">
      <Property name="MouseButtonDownAutoRepeat" value="True" />
      <Area>
        <Dim type="LeftEdge">
          <AbsoluteDim value="0" />
        </Dim>
        <Dim type="TopEdge">
          <AbsoluteDim value="0" />
        </Dim>
        <Dim type="Width">
           <ImageDim imageset="Button1" image="BtnHelp_Normal" dimension="Width" />
        </Dim>
        <Dim type="Height">
          <ImageDim imageset="Button1" image="BtnHelp_Normal" dimension="Height" />
        </Dim>
      </Area>
      <Property name="UseStandardImagery" value="False" />
      <Property name="NormalImage" value="set:Button1 image:BtnHelp_Normal" />
      <Property name="HoverImage" value="set:Button1 image:BtnHelp_Normal" />
      <Property name="PushedImage" value="set:Button1 image:BtnHelp_Normal" />
    </Child>
    <Child type="TLBB_ButtonNULL" nameSuffix="__auto_decbtn__">
      <Property name="MouseButtonDownAutoRepeat" value="True" />
      <Property name="ClickSound" value="-1" />
      <Area>
        <Dim type="LeftEdge">
          <UnifiedDim scale="1.0" type="Width">
            <DimOperator op="Subtract">
              <ImageDim imageset="Button1" image="BtnHelp_Normal" dimension="Width" />
            </DimOperator>
          </UnifiedDim>
        </Dim>
        <Dim type="TopEdge">
          <AbsoluteDim value="0" />
        </Dim>
        <Dim type="Width">
          <ImageDim imageset="Button1" image="BtnHelp_Normal" dimension="Width" />
        </Dim>
        <Dim type="Height">
          <ImageDim imageset="Button1" image="BtnHelp_Normal" dimension="Height" />
        </Dim>
      </Area>
      <Property name="UseStandardImagery" value="False" />
      <Property name="NormalImage" value="set:Button1 image:BtnHelp_Normal" />
      <Property name="HoverImage" value="set:Button1 image:BtnHelp_Normal" />
      <Property name="PushedImage" value="set:Button1 image:BtnHelp_Normal" />
    </Child>
    <Child type="TLBB_EditBoxNormal" nameSuffix="__auto_editbox__">
      <Area>
        <Dim type="LeftEdge">
           <ImageDim imageset="Button1" image="BtnHelp_Normal" dimension="Width" />
        </Dim>
        <Dim type="TopEdge">
            <AbsoluteDim value="0" />
          </Dim>
        <Dim type="RightEdge">
          <UnifiedDim scale="1.0" type="Width">
            <DimOperator op="Subtract">
              <ImageDim imageset="Button1" image="BtnHelp_Normal" dimension="Width" />
            </DimOperator>
          </UnifiedDim>
        </Dim>
        <Dim type="Height">
          <ImageDim imageset="Button1" image="BtnHelp_Normal" dimension="Height" />
        </Dim>
      </Area>
      <Property name="MouseButtonDownAutoRepeat" value="True" />
    </Child>
    <StateImagery name="Enabled">
      <Layer>
        <Section look="TLBB_Look_Common" section="Frame_Type2" />
        <Section look="TLBB_Look_Common" section="Background_Frame_Type1" />
      </Layer>
    </StateImagery>
    <StateImagery name="Disabled">
      <Layer>
        <Section look="TLBB_Look_Common" section="Frame_Type2" />
        <Section look="TLBB_Look_Common" section="Background_Frame_Type1" />
      </Layer>
    </StateImagery>
  </WidgetLook>
</Falagard>