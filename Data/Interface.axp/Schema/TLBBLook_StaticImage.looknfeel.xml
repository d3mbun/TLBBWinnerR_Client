<?xml version="1.0" ?>
<Falagard>
  <WidgetLook name="TLBB_Look_StaticImage">
    <StateImagery name="Enabled" />
    <StateImagery name="Disabled" />
    <StateImagery name="EnabledFrame" />
    <StateImagery name="DisabledFrame" />
    <StateImagery name="EnabledBackground" />
    <StateImagery name="DisabledBackground" />
  </WidgetLook>
	
  <WidgetLook name="TLBB_Look_StaticImageScrollInfo">
    <StateImagery name="Enabled" >
	 <Layer>
        <Section look="TLBB_Look_Common" section="Frame_Type_ScrollInfo" />
     </Layer>	
	</StateImagery>
    <StateImagery name="Disabled" />
    <StateImagery name="EnabledFrame" />
    <StateImagery name="DisabledFrame" />
    <StateImagery name="EnabledBackground" />
    <StateImagery name="DisabledBackground" />
  </WidgetLook>
  
  <WidgetLook name="TLBB_Look_StaticImageScrollInfoFixed">
    <StateImagery name="Enabled" >
